<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h1>Hi <%=user.name%></h1>
    <h2>This is the dashboard page</h2>
    <h1> list of titles </h1>
    <% contents.forEach(function(content){ %>
        <h2> title : <%= content.title %> </h2>
        <p>
            <a href="/<%= content._id %>" class="btn btn-primary">More Info</a>
        </p>
      <% }); %>
      <p>
          <a href="/createContent" class="btn btn-primary">Create Content</a>
      </p>

    <a class="btn btn-light btn-lg" href="/logout" role="button">Log Out</a>
  </body>
</html>
